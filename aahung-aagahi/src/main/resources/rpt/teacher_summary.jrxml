	<queryString>
		<![CDATA[SELECT teacher_name,teacher_id,l.school_name as school_name,gender,(year(current_date())-year(pd.dob)) as age,GROUP_CONCAT( distinct td.school_level
            SEPARATOR ',') as school_level, sum(if(td.school_level='primary',1,0)) as primary_attended,sum(if(td.school_level='secondary',1,0)) as Secondary_attended,teaching_years,
subject_taught,education_level,pmn2.monitoring_score_pct as monitoring_score_pct_pmn1,pmn1.monitoring_score_pct as monitoring_score_pct_pmn2,smn2.monitoring_score_pct asmonitoring_score_pct_smn1,smn1.monitoring_score_pct as monitoring_score_pct_smn2,province,city,l.parent_orginization_name
FROM aagahi.participant_detail pd
left join training_detail as td on find_in_set(pd.teacher_id,td.participant_id)
left join _lse_primary_monitoring as pmn1 on pmn1.participant_id=pd.teacher_id and pmn1.date_created=(select date_created from _lse_primary_monitoring where location_id=pmn1.location_id order by date_created desc limit 0,1)
left join _lse_secondary_monitoring as smn1 on smn1.participant_id=pd.teacher_id and smn1.date_created=(select date_created from _lse_secondary_monitoring where location_id=smn1.location_id order by date_created desc limit 0,1)
left join _lse_primary_monitoring as pmn2 on pmn2.participant_id=pd.teacher_id and pmn2.date_created=(select date_created from _lse_primary_monitoring where location_id=pmn2.location_id order by date_created desc limit 1,1)
left join _lse_secondary_monitoring as smn2 on smn2.participant_id=pd.teacher_id and smn2.date_created=(select date_created from _lse_secondary_monitoring where location_id=smn2.location_id order by date_created desc limit 1,1)
left join school_detail l on l.location_id=pd.location_id

 where
 find_in_set(l.`province`,$P{province}) and find_in_set(l.`city`,$P{city}) and find_in_set(l.`school_level`,$P{school_level}) and pat=1

group by person_id order by teacher_name]]>
	</queryString>
