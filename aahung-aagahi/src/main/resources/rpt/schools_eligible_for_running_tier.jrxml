	<queryString>
		<![CDATA[SELECT
sd.location_id,
    sd.`school_id`,
    sd.`school_name`,
        sd.`partnership_start_date`,
    	sd.`school_level`,
        sd.`school_tier`,
        sd.school_category_new,

if(school_level='Primary',pmn2.date_created,smn2.date_created) as date_created2,
if(school_level='Primary',pmn2.monitoring_score_pct,smn2.monitoring_score_pct) as monitoring_score_pct2,
if(school_level='Primary',pmn1.date_created,smn1.date_created) as date_created1,
if(school_level='Primary',pmn1.monitoring_score_pct,smn1.monitoring_score_pct) as monitoring_score_pct1,


if(if(school_level='Primary',pmn1.monitoring_score_pct,smn1.monitoring_score_pct)>70 and if(school_level='Primary',pmn2.monitoring_score_pct,smn2.monitoring_score_pct)>70 ,'yes','No' ) as eligible


FROM `aagahi`.`school_detail` sd


left join _lse_primary_monitoring_new as pmn1 on sd.location_id=pmn1.location_id and pmn1.date_created=(select date_created from _lse_primary_monitoring_new where location_id=pmn1.location_id order by date_created desc limit 0,1)
left join _lse_secondary_monitoring_new as smn1 on sd.location_id=smn1.location_id and smn1.date_created=(select date_created from _lse_secondary_monitoring_new where location_id=smn1.location_id order by date_created desc limit 0,1)
left join _lse_primary_monitoring_new as pmn2 on sd.location_id=pmn2.location_id and pmn2.date_created=(select date_created from _lse_primary_monitoring_new where location_id=pmn2.location_id order by date_created desc limit 1,1)
left join _lse_secondary_monitoring_new as smn2 on sd.location_id=smn2.location_id and smn2.date_created=(select date_created from _lse_secondary_monitoring_new where location_id=smn2.location_id order by date_created desc limit 1,1)

left join _lse_mt_eligibility_criteria_assessment as meca on meca.location_id=sd.location_id




where date(sd.`partnership_start_date`) between date($P{start_date}) and date($P{end_date}) and
find_in_set(sd.`province`,$P{province}) and find_in_set(sd.`city`,$P{city}) and find_in_set(sd.`school_level`,$P{school_level})and sd.school_category_new='Implementation >1 Cycle' and sd.school_tier='New'

group by sd.location_id;]]>
	</queryString>
