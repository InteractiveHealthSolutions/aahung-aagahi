	<queryString>
		<![CDATA[SELECT
    teacher_name,teacher_id,school_name,school_level,gender,
    eca.`candidate_program_training`,
    eca.`candidate_program_nomination`,
    eca.`mt_eligibility_score_pct`,
    eca.`mt_eligible`
FROM `aagahi`.`mt_eligibility_criteria_assessment` eca
left join participant_detail pd on pd.teacher_id=eca.participant_id
left join school_detail sd on sd.location_id=eca.location_id
where

 find_in_set(sd.`province`,$P{province}) and find_in_set(sd.`city`,$P{city}) and find_in_set(sd.`school_level`,$P{school_level})

order by school_name;]]>
	</queryString>
