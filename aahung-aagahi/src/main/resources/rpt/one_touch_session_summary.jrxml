	<queryString>
		<![CDATA[SELECT
    `lse_one_touch_session_detail`.`form_date`,
    `lse_one_touch_session_detail`.`province`,
    `lse_one_touch_session_detail`.`city`,
    `lse_one_touch_session_detail`.`institution_session_conducted`,
    (select group_concat(username) from users where find_in_set(user_id,`lse_one_touch_session_detail`.`trainer`)) as trainer,
    `lse_one_touch_session_detail`.`session_topic`,
    session_topic_other,
   participants_sex,

    `participants_age_group`,
    `event_attendant`,
    `lse_one_touch_session_detail`.`event_attendant_other`,
    `lse_one_touch_session_detail`.`training_days`,
    female_count+male_count+other_sex_count as total_participants
FROM `aagahi`.`lse_one_touch_session_detail`
where
form_date between $P{start_date} and $P{end_date} and find_in_set(`province`,$P{province}) and find_in_set(`city`,$P{city})
order by form_date;]]>
	</queryString>
