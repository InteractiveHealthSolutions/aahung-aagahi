	<queryString>
		<![CDATA[SELECT
    `comms_training_details_communications`.`form_date`,
    `comms_training_details_communications`.`city`,
    `comms_training_details_communications`.`city_other`,
    (select group_concat(username) from users where find_in_set(user_id,`comms_training_details_communications`.`trainer`)) as trainer
,
    `comms_training_details_communications`.`training_venue`,
    `comms_training_details_communications`.`training_venue_other`,
    `comms_training_details_communications`.`training_days`,
    `comms_training_details_communications`.`topic_covered`,
    `comms_training_details_communications`.`topic_covered_other`,
    `comms_training_details_communications`.`event_attendant`,
    `comms_training_details_communications`.`event_attendant_other`,
    `comms_training_details_communications`.`journalist_count`,
    `comms_training_details_communications`.`blogger_count`,
    `comms_training_details_communications`.`screenwriter_count`,
    `comms_training_details_communications`.`other_media_count`,
    `comms_training_details_communications`.`other_attendant_count`
FROM `aagahi`.`comms_training_details_communications`
 where
form_date between $P{start_date} and $P{end_date} and find_in_set(`city`,$P{city})

 order by form_date;]]>
	</queryString>
