	<queryString>
		<![CDATA[select form_date as training_date,id.institution_name,pd.teacher_name,t.province,t.city,
t.topic_covered,t.topic_covered_other,(if(university_student_count is null,0,university_student_count)+
if(parent_count is null,0,parent_count)+
if(community_leader_count is null,0,community_leader_count)+
if(adolescent_youth_count is null,0,adolescent_youth_count)+
if(children_count is null,0,children_count)+
if(other_attendant_count is null,0,other_attendant_count)) as participant_by_type,(if(male_count is null,0,male_count) + if(female_count is null,0,female_count)+ if(other_sex_count is null,0,other_sex_count)) as participant_by_sex,
participants_age_group


  from
(SELECT form_date,uuid,location_id,participant_id,province,
city,
event_attendant,
event_attendant_other,
university_student_count,
parent_count,
community_leader_count,
adolescent_youth_count,
children_count,
other_attendant_count,
participants_sex,
female_count,
male_count,
other_sex_count,
participants_age_group,
topic_covered,
topic_covered_other FROM srhm_general_step_down_training_details
union
SELECT form_date,uuid,location_id,participant_id,province,
city,
event_attendant,
event_attendant_other,
university_student_count,
parent_count,
community_leader_count,
adolescent_youth_count,
children_count,
other_attendant_count,
participants_sex,
female_count,
male_count,
other_sex_count,
participants_age_group,
topic_covered,
topic_covered_other FROM srhm_amplify_change_step_down_training_details ) as t

left join institution_detail id on id.location_id=t.location_id
left join participant_detail pd on pd.teacher_id=t.participant_id
JOIN numbers n
ON n.num <=  100

where
date(`form_date`) between date($P{start_date}) and date($P{end_date}) and
find_in_set(t.`province`,$P{province}) and find_in_set(t.`city`,$P{city})
group by t.uuid]]>
	</queryString>
