	<queryString>
		<![CDATA[SELECT
    `stakeholder_meetings`.`form_date`,
    `stakeholder_meetings`.`province`,
    `stakeholder_meetings`.`city`,
    `stakeholder_meetings`.`meeting_venue`,
    (SELECT
                GROUP_CONCAT(`users`.`username`
                        SEPARATOR ',')
            FROM
                users
            WHERE
                FIND_IN_SET(users.user_id,`stakeholder_meetings`.`aahung_staff`))
    as `aahung_staff`,
    `stakeholder_meetings`.`event_attendant`,
    `stakeholder_meetings`.`event_attendant_other`,
    `stakeholder_meetings`.`meeting_purpose`,
    `stakeholder_meetings`.`session_topic`,
    `stakeholder_meetings`.`session_topic_other`
FROM `aagahi`.`stakeholder_meetings`

 where
date(form_date) between date($P{start_date}) and date($P{end_date}) and
 find_in_set(`province`,$P{province}) and find_in_set(`city`,$P{city})
 order by form_date]]>
	</queryString>
