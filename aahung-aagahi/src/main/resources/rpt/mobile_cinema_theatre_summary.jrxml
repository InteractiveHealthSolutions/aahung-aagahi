	<queryString>
		<![CDATA[SELECT
    `comms_mobile_cinema_theatre_details`.`form_date`,
    `comms_mobile_cinema_theatre_details`.`province`,
    `comms_mobile_cinema_theatre_details`.`city`,
    `comms_mobile_cinema_theatre_details`.`screening_type`,
    `comms_mobile_cinema_theatre_details`.`topic_covered`,
    `comms_mobile_cinema_theatre_details`.`topic_covered_other`,
    `comms_mobile_cinema_theatre_details`.`performance_title`,
    `comms_mobile_cinema_theatre_details`.`participants_sex`,
    `comms_mobile_cinema_theatre_details`.`female_count`,
    `comms_mobile_cinema_theatre_details`.`male_count`,
    `comms_mobile_cinema_theatre_details`.`other_sex_count`,
    `comms_mobile_cinema_theatre_details`.`participants_age_group`,
    `comms_mobile_cinema_theatre_details`.`age_5_to_10_count`,
    `comms_mobile_cinema_theatre_details`.`age_11_to_15_count`,
    `comms_mobile_cinema_theatre_details`.`age_16_to_20_count`,
    `comms_mobile_cinema_theatre_details`.`age_21_to_49_count`,
    `comms_mobile_cinema_theatre_details`.`age_50_plus_count`,

(if(female_count is null,0,female_count)+if(male_count is null,0,male_count)+if(other_sex_count is null,0,other_sex_count)) as total_participant

FROM aagahi.comms_mobile_cinema_theatre_details
 where
date(form_date) between date($P{start_date}) and date($P{end_date}) and find_in_set(`province`,$P{province}) and find_in_set(`city`,$P{city})

 order by form_date;]]>
	</queryString>
