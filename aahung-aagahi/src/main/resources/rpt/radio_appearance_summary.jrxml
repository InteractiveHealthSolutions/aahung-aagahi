	<queryString>
		<![CDATA[SELECT
    `comms_radio_appearance`.`form_date`,
     dayname(`comms_radio_appearance`.`form_date`) as day_radio_show,
    `comms_radio_appearance`.`time_radio_show`,
    `comms_radio_appearance`.`radio_channel_name`,
    `comms_radio_appearance`.`city`,
    `comms_radio_appearance`.`city_other`,
	(select group_concat(username) from users where find_in_set(user_id,`comms_radio_appearance`.`aahung_staff_appearance`)) as aahung_staff_appearance,
    `comms_radio_appearance`.`topic_covered`,
    `comms_radio_appearance`.`topic_covered_other`,

    `comms_radio_appearance`.`live_call_count`,
    `comms_radio_appearance`.`listener_count`
FROM `aagahi`.`comms_radio_appearance`
 where
form_date between $P{start_date} and $P{end_date}
 order by form_date;]]>
	</queryString>
