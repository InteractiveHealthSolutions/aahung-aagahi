	<queryString>
		<![CDATA[SELECT sdtm.form_date,school_name,teacher_name,teacher_id,mt_eligibility_score_pct,mt_mock_score_pct,d.definition as school_level,d1.definition as program_type,
if(csa_mt_count is not null,csa_mt_count,lsbe_mt_count) as mt_count,don.username,sdtm.monitor,mt_sd_training_score_pct
FROM aagahi.lse_step_down_training_monitoring sdtm
left join school_detail sd on sd.location_id=sdtm.location_id
left join participant_detail pd on pd.person_id=sdtm.participant_id
left join _lse_mt_eligibility_criteria_assessment mteca on mteca.participant_id=sdtm.participant_id
left join _lse_mt_mock_session_evaluation mtmse on mtmse.participant_id=sdtm.participant_id
left join definition d on d.definition_id=sdtm.school_level
left join definition d1 on d1.definition_id=sdtm.program_type
left join (SELECT t1.form_id,(SELECT
                GROUP_CONCAT(`users`.`username`
                        SEPARATOR ',')
            FROM
                users
            WHERE
                FIND_IN_SET(users.user_id,t1.monitor)) AS username FROM lse_step_down_training_monitoring t1
                group by t1.form_id) don on don.form_id=sdtm.form_id




 where
 date(sdtm.form_date) between date($P{start_date}) and date($P{end_date}) and
 find_in_set(sd.`province`,$P{province}) and find_in_set(sd.`city`,$P{city}) and find_in_set(sd.`school_level`,$P{school_level})

group by sdtm.form_id order by sdtm.form_date;]]>
	</queryString>
