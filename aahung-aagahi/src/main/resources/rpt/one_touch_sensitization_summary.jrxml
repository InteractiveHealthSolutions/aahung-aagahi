	<queryString>
		<![CDATA[SELECT form_date,institution_sensitization_session_conducted,(select group_concat(donor_name) from donor where find_in_set(donor_id,srhm_one_touch_sensitization_session_details.donors)) as donors,
province, city,training_days,topic_covered,topic_covered_other,participants_sex,participants_age_group,event_attendant,event_attendant_other,
(if(female_count is null,0,female_count)+if(male_count is null,0,male_count)+if(other_sex_count is null,0,other_sex_count)) as total_participant,
(select group_concat(username) from users where find_in_set(user_id,srhm_one_touch_sensitization_session_details.`trainer`)) as trainer

 FROM aagahi.srhm_one_touch_sensitization_session_details
 where
date(form_date) between date($P{start_date}) and date($P{end_date}) and find_in_set(`province`,$P{province}) and find_in_set(`city`,$P{city})

 order by form_date;]]>
	</queryString>
